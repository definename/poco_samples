<AppConfig>
	<RemoteGen>
		<files>
			<include>
				${POCO_BASE}/include/Poco/RemotingNG/RemoteObject.h
				${POCO_BASE}/include/Poco/RemotingNG/Proxy.h
				${POCO_BASE}/include/Poco/RemotingNG/Skeleton.h
				${POCO_BASE}/include/Poco/RemotingNG/EventSubscriber.h <!-- Remote events support on client side -->
				../TimeServer/include/Types.h
				../TimeServer/include/TimeService.h
			</include>
			<exclude>
			</exclude>
		</files>
		<output>
			<include>include</include>
			<src>src</src>
			<copyright>Copyright (c) 2006-2016, Applied Informatics Software Engineering GmbH.</copyright>
			<mode>client</mode>
			<includeRoot>include</includeRoot>
			<flatIncludes>false</flatIncludes>
			<timestamps>false</timestamps>
		</output>
		<compiler id="msvc">
			<exec>cl</exec>
			<options>
				/I"${POCO_BASE}\include"
				/I".\include"
				/I"..\TimeServer\include"
				/nologo
				/C
				/P
				/TP
			</options>
		</compiler>
		<compiler id="gcc">
			<exec>g++</exec>
			<options>
				-I${POCO_BASE}/include
				-I./include
				-I../TimeServer/include
				-E
				-C
				-DPOCO_NO_GCC_API_ATTRIBUTE
				-o%.i
			</options>
		</compiler>
		<compiler id="clang">
			<exec>clang++</exec>
			<options>
				-I${POCO_BASE}/include
				-I./include
				-I../TimeServer/include
				-E
				-C
				-xc++
				-DPOCO_NO_GCC_API_ATTRIBUTE
				-o%.i
			</options>
		</compiler>
	</RemoteGen>
</AppConfig>
