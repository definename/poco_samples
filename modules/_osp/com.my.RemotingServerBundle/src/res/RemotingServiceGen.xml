				${POCO_ROOT}/include/Poco/RemotingNG/Skeleton.h
				${SOURCE_DIR}/include/RemotingService.h
			</include>
		</files>
		<output>
			<include>${OUTPUT_DIR}/include</include>
			<src>${OUTPUT_DIR}/src</src>
			<copyright>Copyright (c) 2018-2019, Carrot Farm</copyright>
			<mode>server</mode>
		</output>
